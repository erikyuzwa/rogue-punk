{
  "name": "@erikyuzwa/rogue-punk",
  "version": "0.3.5",
  "description": "a small set of 2D game library on top of rot-js",
  "author": "Erik Yuzwa <erikyuzwa@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist-es/index.js",
  "scripts": {
    "build": "tsc && tsc -p tsconfig-es.json",
    "clean": "rm -rf dist dist-es",
    "lint": "tslint -c tslint.json 'lib/**/*.ts'",
    "prepublish": "yarn clean && yarn lint && yarn build"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "rot-js": "^2.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist",
    "dist-es"
  ],
  "publishConfig": {
    "access": "public"
  }
}
